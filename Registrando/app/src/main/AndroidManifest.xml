<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dayana.registrando">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppThemeNoActionBar">

        <activity
            android:name=".Inicial"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>

        <activity
            android:name=".Principal"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.example.dayana.registrando.ValidaRegistro"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.example.dayana.registrando.InformacaoTecnica"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.example.dayana.registrando.RegistroDeEnfermagem"
            android:screenOrientation="portrait" />


    </application>
</manifest>
